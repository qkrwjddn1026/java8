package sec1;
//변수(Variable) : 컴퓨터는 작업에 필요한 데이터를 메모리에 저장하게 되는데 개발자가 프로그램을 처리하기 위해
//필요한 데이터를 중간 중간에 메모리에 저장해야 한다. 그러나 그 위치를 모르면, 저장된 값을 가져 갈 수 없으므로, 
//자료의 저장 위치를 나타내는 이름이 필요한데 그 것을 변수명이라 한다.
public class Var01 {
	public static void main(String[] args) {		
		//변수 선언 : 기억장소를 선언하는 것으로 변수명과 자료형이 같이 정의되어야 함
		int a=10, b=20, c;	//정수형 변수
		char d='a', e;			//문자 변수(''로한글자만 가능)
		String f="aaa", g, k;		//문자열 변수("")
		float h=3.14f, i=0.25f;			//실수형 변수		
	}

}
