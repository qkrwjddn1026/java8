package sec1;

public class Exception4 {
	static class Animal{}
	static class Dog extends Animal{}
	static class Cat extends Animal{}
	public static void main(String[] args){
		Dog dog = new Dog();
		changeDog(dog);
		
		Cat cat = new Cat();
		changeCat(cat);
	}
	public static void changeDog(Animal animal){
		Dog dog = (Dog) animal;
	}
}
