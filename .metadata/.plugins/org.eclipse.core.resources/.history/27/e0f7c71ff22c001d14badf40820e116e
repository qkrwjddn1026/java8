package sec1;
// 프로그램(Operation)의 모든 명령은 Op-code와 Operand로 이루어짐
// a = a + b;
// + : 연산(명령) -> Op-code
// a , b -> Operand
public class Oper1 {

	public static void main(String[] args) {
		int a=20, b=30;
		System.out.println(a); // 제 1 주소 명령어
		prt();				   // 제 0 주소 명령어

	}

	public static void prt(){
		System.out.println("data");
	}
	
}
