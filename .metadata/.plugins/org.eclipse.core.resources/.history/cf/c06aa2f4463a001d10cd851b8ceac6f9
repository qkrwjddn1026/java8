package sec1;

public class Car {
	String compony;
	String model;
	private String color;
	private int maxSpeed;
	int speed=200; //초기값 설정
	int rpm;
	Body body;
	Engine engine;
	Tire tire;
	
	//[접근제한자] [수정자] 데이터타입 필드명;
	
	//맴버상수 (값이 바뀌지 않음)
	//final static 데이터타입 상수명 = 값;
	
	public Car(){}
	public Car(String compony){this.compony=compony;}		
	public Car(String compony, String model) {
		this(compony,model,"gold");
	}
	
	public Car(String compony, String model, String color) {
		super();
		this.compony = compony;
		this.model = model;
		this.color = color;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public int getMaxSpeed() {
		return maxSpeed;
	}
	public void setMaxSpeed(int maxSpeed) {
		this.maxSpeed = maxSpeed;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
		
	}
	public Body getBody() {
		return body;
	}
	public void setBody(Body body) {
		this.body = body;
	}
	public Engine getEngine() {
		return engine;
	}
	public void setEngine(Engine engine) {
		this.engine = engine;
	}
	public Tire getTire() {
		return tire;
	}
	public void setTire(Tire tire) {
		this.tire = tire;
	}
	
	//[접근제한자] 반환타입 메서드명([매개변수타입] [매개변수명],...){
	//	실행할 내용;
	//	return 반환변수 또는 값;		->반환변수가 가지는 값이다 반환되는 값의 종류가 반환타입과 일치해야함.
	//	반환을 하지 않는 경우는 반환타입이 void }
	
	
	//생성자 함수
	//[접근제한자] 클래스명 ([매개변수타임] [매개변수명],....){
	//	this.필드명 = 매개변수명;
	//	this([매개변수명] | 값); }
}
