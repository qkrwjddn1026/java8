package sec3;

public class MotoCycle {
	Tire frontTire = new Tire(8,"앞");
	Tire backTire = new Tire(3,"뒤");
	
	public MotoCycle(){}
	
	public int run(){
		System.out.println("출발");
		if(frontTire == false){ stop(); return 1;}
		if(backTire == false){stop(); return 2;}
		return 0;
	}
	public void stop(){
		System.out.println("자동차가 멈춥니다.");
	
	
}
