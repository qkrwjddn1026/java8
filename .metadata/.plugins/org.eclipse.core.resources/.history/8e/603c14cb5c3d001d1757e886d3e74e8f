package sec1;

public class ChildEx1 {

	public static void main(String[] args) {
		Child c1 = new Child();
		c1.field1 = 10;		//부모 클래스로 부터 물려 받은 field1
		c1.field2 = "kim";  //부모 클래스로 부터 물려 받은 field2
		c1.field3 = 20;		//자식 클래스에서 새로 부여된 field3
		
		//자식 클래스는 부모 요소를 모두 활용할 수 있지만, 부모 클래스는 자식 클래스에만 있는 요소를 활용할 수 없다.
		
		Parent c2 = new Parent();
		c2.field1 = 100;
		c2.field2 = "lee";
		//c2.filed3 = 200;  -> field3는 자식클래스에 있는 것이므로 활용할 수 없다.
		
		//부모 클래스로 부터 유도된 객체는 부모의 생성자 또는 자식의 생성자를 활용하여 객체를 생성할 수 있다.
		Parent c3Parent;
		c3 = new Parent();
		
		
	}

}
