package sec1;

public class Oper5 {
	public static void main(String[] args) {
		// 비트(2진수) 연산
		int a=10, b=20, c=30, d=40, e, f, g, h;
		e = c & d;
		f = c | d;
		g = ~(c & d);
		System.out.println("c & d => " +e);         //c,d 모두 2진수로 : c=011110 d=101000 2의3승자리가 &이기 때문에 값은 8
		System.out.println("c | d => "+ f);         //
		System.out.println("~(c & d) => "+ g);
		
	}

}
