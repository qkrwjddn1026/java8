package sec1;
// 키와 값을 공통으로 갖거나 두 개의 클래스로 일반화 클래스를 만드는 경우
// MAP 구조를 상속받아서도 가능
public class ClassEx2 {

	public static void main(String[] args) {
		Moving<Car,Tire> m1 = new Moving<>();
		m1.key= "age" ;
		m1.value = 40;

	}

}
class Moving<K, V>{	//key, value 를 타입에서 활용 각능함(=Dictionary)
	public K key;
	public V value;
}
class Car{
	
}
class Tire{
	
}
